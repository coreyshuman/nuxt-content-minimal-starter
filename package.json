{
  "name": "nuxt-content-minimal-starter",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "typecheck": "npx nuxi typecheck",
